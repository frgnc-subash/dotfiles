{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "source": "~/.config/fastfetch/images/ryuzaki.png",
        "type": "kitty-icat",
        "height": 11,
        "width": 24,
        "padding": {
            "top": 0
        }
    },
    "display": {
        "separator": " -> "
    },
    "modules": [
        "break",
        {
            "type": "title",
            "keyWidth": 10,
            "format": "         {6}{7}{8}"
        },
        {
            "type": "custom",
            "format": " ─────────────────────────── "
        },
        {
            "type": "kernel",
            "key": " ",
            "keyColor": "yellow"
        },
        {
            "type": "wm",
            "key": "󱂬 ",
            "keyColor": "cyan"
        },
        {
            "type": "shell",
            "key": "󰞷 ",
            "keyColor": "red"
        },
        {
            "type": "cpu",
            "key": "󰻠 ",
            "keyColor": "blue"
        },
        {
            "type": "memory",
            "key": "󰍛 ",
            "keyColor": "magenta",
            "format": "{1} / {2}"
        },
        {
            "type": "disk",
            "key": "󰋊 ",
            "folders": "/",
            "keyColor": "green"
        },
        {
            "type": "uptime",
            "key": "󰅐 ",
            "keyColor": "yellow"
        },
        {
            "type": "command",
            "key": " ",
            "text": "birth_install=$(stat -c %W /); current=$(date +%s); time_progression=$((current - birth_install)); days_difference=$((time_progression / 86400)); echo $days_difference days old",
            "keyColor": "magenta"
        },
        {
            "type": "custom",
            "format": " ─────────────────────────── "
        },
        {
            "type": "custom",
            "format": "   \u001b[31m  \u001b[32m  \u001b[33m  \u001b[34m  \u001b[35m  \u001b[36m  \u001b[37m  \u001b[90m "
        },
        "break"
    ]
}
