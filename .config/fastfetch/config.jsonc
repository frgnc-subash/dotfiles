{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "padding": {
      "top": 2
    },
    "type": "small"
  },
  "display": {
    "separator": " -> "
  },
  "modules": [
    "break",
    {
      "type": "title",
      "keyWidth": 10,
      "format": "         {6}{7}{8}"
    },
    {
      "type": "custom",
      "format": " ─────────────────────────── "
    },
    {
      "type": "kernel",
      "key": " ",
      "keyColor": "yellow"
    },
    {
      "type": "wm",
      "key": "󱂬 ",
      "keyColor": "cyan"
    },
    {
      "type": "shell",
      "key": "󰞷 ",
      "keyColor": "red"
    },
    {
      "type": "cpu",
      "key": "󰻠 ",
      "keyColor": "blue"
    },
    {
      "type": "memory",
      "key": "󰍛 ",
      "keyColor": "magenta",
      "format": "{1} / {2}"
    },
    {
      "type": "disk",
      "key": "󰋊 ",
      "folders": "/",
      "keyColor": "green"
    },
    {
      "type": "uptime",
      "key": "󰅐 ",
      "keyColor": "yellow"
    },
    {
      "type": "custom",
      "format": " ─────────────────────────── "
    },
    {
      "type": "custom",
      "format": "   \u001b[31m  \u001b[32m  \u001b[33m  \u001b[34m  \u001b[35m  \u001b[36m  \u001b[37m  \u001b[90m "
    },
    "break"
  ]
}
